{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "headers": {
          "Content-Security-Policy": "default-src 'self'; script-src 'self' https://vercel.live"
        }
      }
    ]
  }
  